    "@prisma/client": "^6.6.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "prisma": "^6.6.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaposoG/botzin.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/RaposoG/botzin/issues"
  },
  "homepage": "https://github.com/RaposoG/botzin#readme",
  "pnpm": {
